{
    "name": "uupt-open/mini-wechat",
    "alias": "微信小程序模块",
    "description": "微信登录、授权、支付相关功能",
    "type": "library",
    "version": "1.0.0",
    "keywords": [
        "owl-admin",
        "extension"
    ],
    "homepage": "https://github.com/uupt/mini-wechat",
    "license": "MIT",
    "authors": [
        {
            "name": "UU跑腿",
            "email": "admin@uupt.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "slowlyo/owl-admin": "*",
        "w7corp/easywechat": "6.15.0",
        "ext-openssl": "*"
    },
    "autoload": {
        "psr-4": {
            "Uupt\\MiniWechat\\": "src/"
        }
    },
    "extra": {
        "owl-admin": "Uupt\\MiniWechat\\MiniWechatServiceProvider",
        "laravel": {
            "providers": [
                "Uupt\\MiniWechat\\MiniWechatServiceProvider"
            ]
        }
    }
}
